<table cellpadding="0" cellspacing="0" border="0" id="[ID]" [StyleClass:class="[]"] [Style:style="[]"]>
	<tr>
		[@ColumnHeaders:
			<th id="[ID]" [StyleClass:class="[]"] [Style:style="[]"]>[TextValue]</th>
		]
	</tr>
	[@Rows:
		<tr>
			[@Columns:
				<td id="[ID]" [StyleClass:class="[]"] [Style:style="[]"]>[ItemRender]</td>
			]
		</tr>
	]
</table>
<script type="text/javascript" charset="utf-8">
	SetComponent( '[ID]', ComponentTable( '[ID]' ) );
	_( '[ID]' ).activate();
</script>

