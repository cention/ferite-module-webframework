<ul id="[ID]"> [@Items: []] </ul>
<input type="hidden" class="debug" name="[ID]" value="" id="FormValue_[ID]" />
<script type="text/javascript" charset="utf-8">
	SetComponent('[ID]', ComponentList('[ID]',[multiple:true][!multiple:false]));
	[@Selected: _('[.ID]').selectItemsByValue('[]'); ]
	_('[ID]').activate();
</script>
