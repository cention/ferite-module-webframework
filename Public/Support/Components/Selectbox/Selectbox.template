<select id="[ID]" size="[size]" multiple="yes" [StyleClass:class="[]"] [Style:style="[]"]>
	[@Items: []]
</select>
<input type="hidden" class="debug" name="[ID]" value="" id="FormValue_[ID]" />
<script type="text/javascript" charset="utf-8">
	SetComponent('[ID]', ComponentSelectbox('[ID]'));
	[@Selected: _('[.ID]').selectItemsByValue('[]');]
	_('[ID]').activate();
</script>
